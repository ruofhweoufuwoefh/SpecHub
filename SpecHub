Main.Name = "Main"
Main.Parent = SpecR
Main.BackgroundColor3 = Color3.fromRGB(248, 0, 66)
Main.BorderSizePixel = 0
Main.Position = UDim2.new(0.20286198, 0, 0.147386819, 0)
