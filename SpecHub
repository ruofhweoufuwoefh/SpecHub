end)

Hide.Name = "Hide"
Hide.Parent = Main
Hide.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Hide.BackgroundTransparency = 1.000
Hide.BorderColor3 = Color3.fromRGB(27, 42, 53)
Hide.Position = UDim2.new(0.940084994, 0, 0.0566573143, 0)
Hide.Size = UDim2.new(0.0199999996, 0, 0.0199999996, 0)
Hide.SizeConstraint = Enum.SizeConstraint.RelativeXX
Hide.ZIndex = 2
Hide.Font = Enum.Font.Code
Hide.Text = "-"
Hide.TextColor3 = Color3.fromRGB(255, 255, 255)
Hide.TextScaled = true
Hide.TextSize = 14.000
Hide.TextWrapped = true
Hide.MouseButton1Down:connect(function()
if ViewFrame.Visible == true then
ViewFrame.Visible = false
Hide.Text = "+"
else
ViewFrame.Visible = true
Hide.Text = "-"
end
end)

