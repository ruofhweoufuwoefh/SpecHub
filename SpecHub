Main.Draggable = true

Close.Name = "Close"
Close.Parent = Main
Close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Close.BackgroundTransparency = 1.000
Close.BorderColor3 = Color3.fromRGB(27, 42, 53)
Close.Position = UDim2.new(0.970254898, 0, 0.0700000003, 0)
Close.Size = UDim2.new(0.0199999996, 0, 0.0199999996, 0)
